pipeline{
    agent any 
    stages{
        stage(" build stage"){
            steps{
                   script{
                    withSonarQubeEnv(credentialsId: 'sonarqb') {
                            sh 'chmod +x gradlew'
                            sh './gradlew sonarqube'
                    }      
                        timeout(1){
                        def qg = waitForQualityGate()
                        if (qg.status != 'OK') {
                            error "Pipeline aborted due to quality gate failure: ${qg.status}"
                        }
                        }
 
            }
        }
    }
}
}